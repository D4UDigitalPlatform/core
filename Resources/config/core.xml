<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="core.config.class">Itkg\Config</parameter>
        <parameter key="core.container.class">Itkg</parameter>
    </parameters>
    <services>
        <service id="core.config" class="%core.config.class%" public="true" />
        <service id="core.container" class="%core.container.class%" public="true" />
    </services>
</container>